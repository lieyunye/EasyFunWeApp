}
.list{
  width: 100%;
  height: 100%;
  background-color: #ff0000;
}
.item {
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  color: #888888;
  font-size: 58rpx;
  height: 500rpx;
  background-color: #ffff00;

}

.kind-list-item {
  margin: 20rpx 0;
  background-color: #FFFFFF;
  border-radius: 4rpx;
  overflow: hidden;
}
.space-right{
  width: 50rpx;
  height: 100%;
  position: fixed;
  right:0rpx;
  top:0rpx;  
  background-color: #f11fff;
  opacity:0;

} 
 .space-left{
  width: 50rpx;
  height: 100%;
  position: fixed;
  left:0rpx;
  top:0rpx;  
